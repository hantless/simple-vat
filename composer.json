{
    "name": "hantless/simple-vat",
    "description": "Simple package to check European's VAT number format",
    "keywords": ["laravel", "check", "vat", "format"],
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Arijs",
            "email": "hantless@gmail.com"
        }
    ],
    "require": {
      "illuminate/support": "~5"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Hantless\\SimpleVat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Hantless\\SimpleVat\\SimplevatServiceProvider"
            ]
        }
    }
}
